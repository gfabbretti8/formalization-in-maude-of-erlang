mod HELP-SYNTAX is

  protecting SYNTAX .

  var EX : Exp .
  var EXSEQ : NeExpSeq .

  op #head : ExpSeq -> ExpSeq .
  eq #head(EX, EXSEQ) = EX, EXSEQ .
  eq #head(EX) = EX .


  op #flatten-list : List -> ExpSeq .
  eq #flatten-list([]) = [] .
  eq #flatten-list([EX]) = EX .
  eq #flatten-list([EX | EXSEQ]) = EX, #flatten-list(EXSEQ) .
  eq #flatten-list(EX) = EX .

endm
